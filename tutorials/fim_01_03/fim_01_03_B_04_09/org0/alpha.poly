FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      alpha.poly;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    "wall.*"
    {
        type            zeroGradient;
    }

    outlet
    {
        type            zeroGradient;
    }

    inlet
    {
        type            fixedValue;
        value           uniform 1;
    }


    frontAndBack
    {
        type            empty;
    }
}

