FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      solidificationProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

shearModulus 200e6;//Pa

viscLimEl 450e3;//Pas

viscLimSolidCo #calc "0.9*$viscLimEl";
